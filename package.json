{
  "name": "dreamworks",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^9.0.1",
    "eslint": "^9.13.0",
    "eslint-webpack-plugin": "^4.2.0",
    "express": "^4.16.4",
    "nodemon": "^3.1.7",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-device-detect": "^2.1.2",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "sass": "^1.38.2",
    "superagent": "^10.1.1",
    "typescript": "^5.6.3",
    "webpack": "5.95.0"
  },
  "scripts": {
    "start": "PORT=5005 react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "yarn --cwd client/app build",
    "server": "nodemon server.js",
    "client-install": "yarn --cwd client/app install",
    "client": "yarn --cwd client/app start",
    "sass": "sass --watch src/assets/sass:src/assets/css",
    "dev": "concurrently \"yarn run start\" \"yarn run sass\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "eslint-config-react-app": "^7.0.1"
  }
}
